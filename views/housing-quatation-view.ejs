                );

                current_date();
            })
            .catch(function (err) {
                console.log('error: ' + err);
            });
        }

        function current_date(){
            var today = new Date();
            var dd = String(today.getDate()).padStart(2, '0');
            var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
            var yyyy = today.getFullYear();

            today = dd + '.' + mm + '.' + yyyy;

            $('#current_date').append(
                '<p>Date: ' + today + '</p>'
            );


        }
        
            
