        function client_details(){
            const url3 = "/quatation/client_data/getdata";
        
            fetch(url3)
            .then(function (response) {
                return response.json();
            })
            .then(function (data) {
                
                $('#client_details').append(
                    '<p>' + data[0].client_name + '</p>' +
